@{
    ViewData["Title"] = "SimplyDoc Docs";
    Layout = null;
}

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Author: Tokić d.o.o." />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/rpaapp.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Raleway" rel="stylesheet">
</head>

<style>
    .nvbr{
        text-decoration: none;
        font-weight: bold;
        font-size: large;
        width: 100%;
        color: lightgrey;
        padding: 5px 22px;
    }
    h3{
        margin-bottom: 22px;
        user-select: none;
    }
    .txt
    {
        color: white;
        padding: 16px;
    }
</style>

<body>
    <div class="row">
        <div class="col-md-2" style="display: flex; position: sticky; top: 0; height: 600px; flex-direction: column;">
            <h3 style="padding-left: 22px; padding-top: 12px; user-select: none;">SimplyDoc API</h3>
            <a class="nvbr" href="#key"> API Key </a>
            <a class="nvbr" href="#confirmed"> List of confirmed documents </a>
            <a class="nvbr" href="#upload"> Upload files </a>
            <a class="nvbr" href="#archive"> Archive file </a>
            <a class="nvbr" href="#problem"> File transfer to problem </a>
            <a class="nvbr" href="#download"> Download PDF </a>
            <a class="nvbr" href="#delete"> Delete document </a>
            <a class="nvbr" href="#docid"> Get RAC number </a>
            <a class="nvbr" href="#metadata"> Get PDF Metadata</a>
            <a class="nvbr" asp-controller="Home" asp-action="Index" style="color:black;">Back to main page</a>
        </div>
        <div class="col-md-10" style="border-left: 1px solid lightgrey; padding-left: 65px;">
            <div id="key" style="padding-bottom: 100px; padding-top: 60px;">
                <h3>API Key</h3>
                <div style="display: flex;">
                    <p class="col-md-6">
                        ApiKey is fixed value that is mandatory for all GET/POST requests. <br>
                        It should be inserted under the Header part of the request.
                    </p>
                    <div class="col-md-6">
                        <div style="padding: 0 16px 16px 16px;">
                            <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 90px;">
                                <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                                <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">API KEY
                                </p>
                                </div>
                                <div>
                                    <p class="txt">Header: "ApiKey“:“********-*****-*****“ //fixed ApiKey</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="confirmed" style="padding-bottom: 100px;">
                <h3>List of confirmed documents</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Get the list of Documents that are in the TAB „Confirmed“ in SimplyDoc. <br>
                    Those documents are already gone through OCR part of the SimplyDoc and are approved by human in the TAB „Ready“ or they come automatically after they where scanned by SimplyDoc.
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 115px;">
                            <div style="height: 22%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">GET /DmsMove
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "content-type“:“application/json“ //for json requests <br>
                                    Header: "ApiKey“:“********-*****-*****“ //fixed ApiKey
                                </p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 820px;">
                        <p class="txt">
                                Status: 200 <br>
                                { <br>
                                    "id" : 1,<br>
                                    "docId" : "64hf2fa2-a1ca-4fee-97a2-9229uj034c14e",<br>
                                    "isReviewed" : true,<br>
                                    "isDownloaded" : false,<br>
                                    "pngNames" : "zasave1.png",<br>
                                    "name" : "Example d.o.o.",<br>
                                    "clientCode" : null,<br>
                                    "group" : "DOM",<br>
                                    "invoiceTypeId" : 4,<br>
                                    "currency" : "EUR",<br>
                                    "invoiceNumber" : "619-3-1",<br>
                                    "invoiceDate" : "2022-04-06T00:00:00",<br>
                                    "bruto" : 14591.06,<br>
                                    "neto" : 11672.85,<br>
                                }<br>
                                {<br>
                                    "id" : 2,<br>
                                    "docId" : "2uf02-3r2-tge-97a2-7450920jfasd1",<br>
                                    "isReviewed" : true,<br>
                                    "isDownloaded" : false,<br>
                                    "pngNames" : "zasave12.png",<br>
                                    "name" : "Example d.o.o.",<br>
                                    "clientCode" : null,<br>
                                    "group" : "INO",<br>
                                    "invoiceTypeId" : 3,<br>
                                    "currency" : "EUR",<br>
                                    "invoiceNumber" : "619-3-1",<br>
                                    "invoiceDate" : "2022-04-06T00:00:00",<br>
                                    "bruto" : 14591.06,<br>
                                    "neto" : 11672.85,<br>
                                }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="upload" style="padding-bottom: 100px;">
                <h3>Upload files</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Uploading files (.pdf) to the SimplyDoc. <br>
                    // Maximum upload size is ~28 MB. <br>
                    // Maximum of 20 files can be uploaded at the same time
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 115px;">
                            <div style="height: 25%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">POST  /pdfs/UploadFiles
                            </p>
                            </div>
                            <div>
                                <p class="txt">files={"files":("Example.pdf",open("FilePath/Example.pdf","rb"),"application/pdf")} <br>
                                Header: "ApiKey“:“********-*****-*****“ //fixed ApiKey
                                </p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Multiple files upload example</b></p>
                    <div style="padding: 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 175px;">
                            <div style="height: 15%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">POST  /pdfs/UploadFiles
                            </p>
                            </div>
                            <div>
                                <p class="txt">files=[ <br>
                                ('files':('Example.pdf',open('FilePath/Example.pdf','rb'),'application/pdf')), <br>
                                ('files':('Example2.pdf',open('FilePath/Example.pdf','rb'),'application/pdf')) <br>
                                ] <br>
                                Header: "ApiKey“:“********-*****-*****“ //fixed ApiKey
                                </p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 100px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="archive" style="padding-bottom: 100px;">
                <h3>Archive file</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Send document from „Confirmed“ Tab to „Archived“ Tab. <br>
                    Attributes: <br>
                    •	Guid = Document Id that is provided by SimplyDoc // Required <br>
                    •	DocId = Unique number that is provided by the app where document is stored or any other unique number by which you can later find certain document. // Required <br>
                    // One at a time.
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">POST  /Archive?gd=guid&rac=DocId
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 100px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="problem" style="padding-bottom: 100px;">
                <h3>File transfer to problem</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Send document from „Confirmed“ Tab to „Problem“ Tab. <br>
                    Attributes: <br>
                    •	Guid = Document Id that is provided by SimplyDoc. // Required <br>
                    •	DocId = Unique number that is provided by the app where document is stored or any other unique number by which you can later find certain document. // Required <br>
                    •	ErrorDesc = Description of the problem (String). // Optional <br>
                    // One at a time. <br>
                    // Since ErrorDesc is an optional parameter, you can pass the Empty String. 
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">POST  /ReportProblem?gd=guid&rac=DocId &desc=ErrorDesc
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 100px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="download" style="padding-bottom: 100px;">
                <h3>Download PDF</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Downloads .pdf file from SimplyDoc. <br>
                    Attributes: <br>
                    •	Guid = Document Id that is provided by SimplyDoc. // Required <br>
                    // One at a time. 
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">GET  /Home/Download?gd=guid
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 100px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="delete" style="padding-bottom: 100px;">
                <h3>Delete document</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Delete document from SimplyDoc.  <br>
                    Attributes: <br>
                    •	Guid = Document Id that is provided by SimplyDoc. // Required <br>
                    // One at a time.
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">GET  /Home/DeleteConfirmed?gd=guid
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 100px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="docid" style="padding-bottom: 100px;">
                <h3>Get RAC number</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Getting DocumentID number from any archived document, by providing documents' GUID. <br>
                    Attributes: <br>
                    •	Guid = Document Id that is provided by SimplyDoc. // Required <br>
                    // One at a time.
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">GET  /GetRac?gd=guid
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 130px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                                "RAC_2022_033579" <br>
                        }
                        </p>
                    </div>
                </div>
                </div>
            </div>
            <div id="metadata" style="padding-bottom: 100px;">
                <h3>Get PDF Metadata</h3>
                <div style="display: flex;">
                <p class="col-md-6">
                    Getting list of pdfs in Confirmed state and their metadata. <br>
                </p>
                <div class="col-md-6">
                    <div style="padding: 0 16px 16px 16px;">
                        <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 90%; height: 100px;">
                            <div style="height: 33%; width: 100%; background-color: #4f566b; border-radius: 10px 10px 0px 0px;">
                            <p style="color: #c1c9d2; font-weight: bold; padding-left: 16px;">GET  /pdfs
                            </p>
                            </div>
                            <div>
                                <p class="txt">Header: "ApiKey“:“********-*****-*****“ //Fixed ApiKey</p>
                            </div>
                        </div>
                    </div>
                    <p style="padding: 30px; margin: auto; width: 90%;"><b>Example response:</b> // Successful</p>
                    <div style="background-color: #707b8f; border-radius: 10px; margin: auto; width: 87%; height: 690px;">
                        <p class="txt">{ <br>
                                Status: 200 <br>
                                    { <br>
                                        "id": 157,<br>
                                        "fname": "test1.pdf",<br>
                                        "guid": "698ff321-8386-42be-86a3-6d0de3768049",<br>
                                        "fsize": 89524,<br>
                                        "isDownloaded": false,<br>
                                        "isUploaded": false,<br>
                                        "uploaded": "2022-06-27T12:11:30.383975",<br>
                                        "writername": " ",<br>
                                        "writer": <br> {<br>
                                            "firstName": null,<br>
                                            "lastName": null,<br>
                                            "fullName": " ",<br>
                                            "id": 0,<br>
                                            "userName": null,<br>
                                            "normalizedUserName": null,<br>
                                            "email": null,<br>
                                            "normalizedEmail": null,<br>
                                            "emailConfirmed": false,<br>
                                            "lockoutEnabled": false,<br>
                                            "accessFailedCount": 0<br>
                                        }<br>
                                    }<br>
                                }
                        </p>
                    </div>
                </div>
                </div>
            </div>
        </div>
    </div>
</body>


<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>